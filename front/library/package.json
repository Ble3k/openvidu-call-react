    "@material-ui/icons": "^2.0.3",
    "axios": "^0.18.0",
    "jquery": "^3.3.1",
    "openvidu-browser": "^2.4.0",
    "typedoc": "^0.5.10",
    "typeface-roboto": "0.0.54"
  }
